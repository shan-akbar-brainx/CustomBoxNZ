//NAV ACCORDION
.nav-accordion {
  width: 100%;
  line-height: 0.5em;
  list-style: none;
  margin: 0;
  padding: 0;
  li {
    background: none;
    position: relative;
    border-bottom: #f4f4f4 solid 1px;
    padding-left: 15px;  
    &.active {
      &:before {
        content: "\f111";
        font-family: fontawesome;
        font-size: 7px;
        color: #181818;
        position: absolute;
        top: 10px;
      }
    } 
    a {
      display: block;
      font-size: 14px;
      font-weight: 400; 
      line-height: 40px;
      margin: 0;
      padding: 0;
      text-align: left;
      text-decoration: none;
      text-shadow: none;   
    }
    span.expand, span.collapse {
      background-image: none;
      cursor: pointer;
      display: block;
      font-size: 0;
      position: absolute;
      text-indent: -9999px;
      top: 11px;
      z-index: 1;
      right: 24px;
      height: auto;
    }
    span.expand {
      &:after {
        content: "\e857";
        font-family: "techmarket";
        font-weight: normal;
        font-style: normal;
        display: inline-block;
        text-decoration: inherit;
        font-size: 19px;
        margin-right: 0; 
        float: right;
        color: white;
        text-indent: 0;
        line-height: 19px;
        padding-right: 5px;
        width: 19px;
        height: 19px;
      }
    }
    span.collapse {
      &:after {
        content: "\e856";
        font-family: "techmarket";
        font-weight: normal;
        font-style: normal;
        display: inline-block;
        text-decoration: inherit;
        font-size: 19px;
        margin-right: 0; 
        float: right;
        color: white;
        text-indent: 0;
        line-height: 19px;
        padding-right: 5px;
        width: 19px;
        height: 19px;
      }
    }
  }
  ul {
    display: none;
    margin: 0;
    li {
      clear: both;
      margin: 0;
      padding: 0;
      border-bottom-width: 0;
      &.active {
        font-weight: bold;
      }
      a { 
        outline: none;
        padding-left: 15px;
      }
    }
    ul {
      li {
        a { 
          padding-left: 25px;
        }
      }
      ul {
        li {
          a {
            padding-left: 30px;
          }
        }
      }
    }
  }
}
