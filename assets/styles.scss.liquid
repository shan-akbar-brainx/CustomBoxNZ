  }
}
.top-right-area {
  padding: 24px 0;
}
.header-link-area {
  display:inline-block; 
  vertical-align: middle;
  line-height:45px;
  padding-left:10px;
  .toplink {
    li { 
      display: inline; 
      font-size: 14px;
      padding: 0 15px;
      border-left: 2px solid #d9e1e1;
      line-height: 19px;
      a {
        font-size:14px;
        font-weight:300;
        color:#444444; 
      }
      &:first-child {
        border:none;
        padding-left:0px;
      }
    }
  }
}
.top-link-area {
  display: inline-block;
  float:right;
  vertical-align: middle;
  line-height:45px;
  .links{    
    > li{
      display: inline; 
      line-height: 15px;
      font-size: 14px;    
      padding:0 15px; 
      border-left: 2px solid #d9e1e1;
      line-height: 19px; 
      &.last {
        padding-right: 0;
      }
      &:first-child{
        border-left:0;
      }
      i { 
        font-size: 21px;
        display: inline-block;
        vertical-align: middle;
      }
    }
  }   
  .currency-wrapper, .language-wrapper {
    display: inline-block;  
    > a{
      display: block;
      width: 100%; 
      line-height: 17px;
      padding: 0 14px 0 0; 
      position:relative;  
      &:hover{
        text-decoration:none;
      }
      i{
        position: absolute;
        top: -1px;
        right: 0;
        width: 14px; 
        font-size: 14px;
        text-align: center;
      } 
    }
  }
  .currency-wrapper {
    position: relative;
    > a{ 
      border-color: transparent;
      &:hover {
        text-decoration: underline;
      }
    }
    .current-currency {
      display: block;
      cursor: pointer;
    }
    &:hover {
      .setting-currency {
        visibility: visible;
        opacity: 1;
        right: 0; 
      }
    }
  }
  .language-wrapper {
    position: relative; 
    border-left: 2px solid #d9e1e1;
    padding-left: 17px;
    margin-left: 15px;
    > a{ 
      border-color: transparent;
      &:hover {
        text-decoration: underline;
      }
    }
    .current-language {
      span {
        display: none;
        cursor: pointer;
        &.active {
          display: block;
        }
        img{
          margin-top: -2px;
          margin-right: 3px;
        }
      }
    }
    &:hover {
      .select-language {
        visibility: visible;
        opacity: 1;
      }
    }
  }
  .select-language, .setting-currency {
    padding: 4px 0;
    position: absolute;
    left: -7px;
    top: 26px;
    z-index: 10;
    background-color: #fff;
    visibility: hidden;
    opacity: 0;
    border-radius:3px;
    -moz-transition: all 0.3s ease-in-out 0s;
    -webkit-transition: all 0.3s ease-in-out 0s;
    -o-transition: all 0.3s ease-in-out 0s;
    -ms-transition: all 0.3s ease-in-out 0s;
    transition: all 0.3s ease-in-out 0s;
    box-shadow: 0 6px 6px rgba(0,0,0,0.2);
    li {
      display: inherit!important;
      float: none;
      a {
        display: block;
        font-size: 14px;
        padding: 3px 9px;
        text-align:left;
        color:#444 !important;
        background:white; 
        &:hover{
          background:#444;
          color:white !important;
        }
      }
      img {
        max-width: 14px; 
        position: relative;
        top: -2px;
      }
    }
  }   
  .select-language{
    width:110px;
    left:7px;
  }
  .setting-currency{
    width:65px;
  }
} 
